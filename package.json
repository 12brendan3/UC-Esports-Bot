{
  "name": "uc-esports-bot",
  "version": "1.1.2",
  "description": "A bot for the University of Cincinnati Esports Discord server.",
  "main": "index.js",
  "scripts": {
    "test": "node index",
    "pretest": "eslint --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/12brendan3/UC-Esports-Bot.git"
  },
  "keywords": [
    "discord",
    "js",
    "discord.js",
    "bot"
  ],
  "author": "Brendan Root, Baru Yogesh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/12brendan3/UC-Esports-Bot/issues"
  },
  "homepage": "https://github.com/12brendan3/UC-Esports-Bot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.9.0",
    "axios": "^0.27.2",
    "bad-words": "^3.0.4",
    "bufferutil": "^4.0.6",
    "cronosjs": "^1.7.1",
    "discord.js": "^13.6.0",
    "emoji-regex": "^10.1.0",
    "html-entities": "^2.3.3",
    "node-gyp": "^9.0.0",
    "nodemailer": "^6.7.5",
    "sequelize": "^6.19.0",
    "sqlite3": "^5.0.7",
    "tweetnacl": "^1.0.3",
    "utf-8-validate": "^5.0.9",
    "youtube-search": "^1.1.6",
    "ytdl-core": "^4.11.0",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "eslint": "^7.32.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2019
    },
    "rules": {
      "brace-style": [
        "error",
        "1tbs"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "comma-spacing": "error",
      "comma-style": "error",
      "curly": [
        "error",
        "multi-line",
        "consistent"
      ],
      "dot-location": [
        "error",
        "property"
      ],
      "handle-callback-err": "off",
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "max-nested-callbacks": [
        "error",
        {
          "max": 4
        }
      ],
      "max-statements-per-line": [
        "error",
        {
          "max": 2
        }
      ],
      "no-console": "off",
      "no-empty-function": "error",
      "no-floating-decimal": "error",
      "no-inline-comments": "error",
      "no-lonely-if": "error",
      "no-multi-spaces": "error",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2,
          "maxEOF": 1,
          "maxBOF": 0
        }
      ],
      "no-shadow": [
        "error",
        {
          "allow": [
            "err",
            "resolve",
            "reject"
          ]
        }
      ],
      "no-trailing-spaces": [
        "error"
      ],
      "no-var": "error",
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "prefer-const": "error",
      "quotes": [
        "error",
        "backtick"
      ],
      "semi": [
        "error",
        "always"
      ],
      "space-before-blocks": "error",
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "space-in-parens": "error",
      "space-infix-ops": "error",
      "space-unary-ops": "error",
      "spaced-comment": "error",
      "yoda": "error"
    }
  }
}
